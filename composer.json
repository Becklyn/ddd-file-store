{
    "name": "becklyn/ddd-file-store",
    "description": "File storage engine used in Becklyn projects",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Marko Vujnovic",
            "email": "mv@becklyn.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "becklyn/ddd-symfony-bridge": "^2.0",
        "doctrine/doctrine-bundle": "^2.4",
        "doctrine/orm": "^2.10",
        "psr/log": "^3.0",
        "ramsey/uuid": "^4.0",
        "symfony/config": "^6.0",
        "symfony/dependency-injection": "^6.0",
        "symfony/filesystem": "^6.0",
        "symfony/http-foundation": "^6.0",
        "doctrine/migrations": "^3.0"
    },
    "require-dev": {
        "phpspec/prophecy-phpunit": "^2.0",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Becklyn\\Ddd\\FileStore\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Becklyn\\Ddd\\FileStore\\Tests\\": "tests/"
        }
    }
}
